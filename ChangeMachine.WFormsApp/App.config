<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--Tipo de log: File, EventLog, Database-->
    <add key="LogType" value="Database"/>
    <add key="LogFilePath" value="C:\\logs"/>
    <add key="LogFileName" value="change.log"/>
  </appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

  <connectionStrings>
    <add name="ChangeMachine" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=ChangeMachine;Integrated Security=true;"/>
  </connectionStrings>
</configuration>